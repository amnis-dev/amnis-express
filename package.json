{
  "name": "@amnis/express",
  "version": "0.1.0",
  "repository": "https://github.com/amnis-dev/amnis-express",
  "author": "Eric Crowell <admin@amnis.dev>",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "build": "pnpm build:types && pnpm build:dist",
    "build:dist": "vite build --mode production",
    "build:types": "tsc -p tsconfig.build.json",
    "test": "vitest --run",
    "lint": "tsc --noEmit && eslint --ext .ts --ext .tsx \"./\"",
    "clean": "rimraf ./dist ./lib ./types ./tsconfig.build.tsbuildinfo ./tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "@amnis/api": "^0.16.6",
    "@amnis/eslint-config-node": "^2.2.4",
    "@amnis/state": "^0.16.11",
    "@amnis/tsconfig-node": "^2.2.4",
    "@reduxjs/toolkit": "alpha",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.13.0",
    "@types/supertest": "^2.0.12",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "cookie-parser": "^1.4.6",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.4",
    "ws": "^8.12.0"
  }
}
